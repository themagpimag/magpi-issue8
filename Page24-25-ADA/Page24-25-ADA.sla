            <ITEXT CH=" to mean the same thing."/>
            <para PARENT="Code explanation text"/>
            <ITEXT CH="Also, "/>
            <ITEXT CPARENT="Inline commands" CH="if Is_Defective = False then"/>
            <ITEXT CH=" and "/>
            <ITEXT CPARENT="Inline commands" CH="if not Is_Defective then"/>
            <ITEXT CH=" mean the same thing."/>
            <para PARENT="Code explanation text"/>
            <ITEXT FONT="Bitstream Charter Bold" CH="Line 8"/>
            <ITEXT CH=": Anything between the "/>
            <ITEXT CPARENT="Inline commands" CH="then"/>
            <ITEXT CH=" and the "/>
            <ITEXT CPARENT="Inline commands" CH="else"/>
            <ITEXT CH=" keywords is run when the condition is true."/>
            <para PARENT="Code explanation text"/>
            <ITEXT FONT="Bitstream Charter Bold" CH="Line 10"/>
            <ITEXT CH=": Anything between the "/>
            <ITEXT CPARENT="Inline commands" CH="else"/>
            <ITEXT CH=" and the "/>
            <ITEXT CPARENT="Inline commands" CH="end if"/>
            <ITEXT CH=" keywords is run when the condition is false."/>
            <para PARENT="Code explanation text"/>
            <ITEXT FONT="Bitstream Charter Bold" CH="Line 11"/>
            <ITEXT CH=": All if statements should end with end if; even if there is no else part."/>
